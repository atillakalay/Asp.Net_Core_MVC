@model MyAspCoreApp.Web.ViewModels.ProductListPartialViewModel

<table class="table table-dark table-striped-columns">
    <thead>
    <tr>
        <th>#</th>
        <th>İsim</th>
        <th>Fiyat</th>
        <th>Stok</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var item in Model.Products)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Price</td>
            <td>@item.Stock</td>
        </tr>
    }
    </tbody>
</table>